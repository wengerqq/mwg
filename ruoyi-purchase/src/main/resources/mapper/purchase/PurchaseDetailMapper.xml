<mapper namespace="com.ruoyi.purchase_detail.mapper.PurchaseDetailMapper">
    
    <resultMap type="PurchaseDetail" id="PurchaseDetailResult">
        <result property="id"    column="id"    />
        <result property="purchaseKeyId"    column="purchase_key_id"    />
        <result property="productSku"    column="product_sku"    />
        <result property="productName"    column="product_name"    />
        <result property="quantity"    column="quantity"    />
        <result property="price"    column="price"    />
        <result property="referencePrice"    column="reference_price"    />
        <result property="taxRate"    column="tax_rate"    />
        <result property="remark"    column="remark"    />
