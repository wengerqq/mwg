        <result property="status"    column="status"    />
        <result property="quantityReceived"    column="quantity_received"    />
        <result property="isShelf"    column="is_shelf"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectPurchaseDetailVo">
        select id, purchase_key_id, product_sku, product_name, quantity, price, reference_price, tax_rate, remark, external_order_no, delivery_time, status, quantity_received, is_shelf, create_time, update_time from purchase_detail
    </sql>

    <select id="selectPurchaseDetailList" parameterType="PurchaseDetail" resultMap="PurchaseDetailResult">
        <include refid="selectPurchaseDetailVo"/>
        <where>  
            <if test="purchaseKeyId != null  and purchaseKeyId != ''"> and purchase_key_id = #{purchaseKeyId}</if>
            <if test="productSku != null  and productSku != ''"> and product_sku = #{productSku}</if>
            <if test="productName != null  and productName != ''"> and product_name like concat('%', #{productName}, '%')</if>
            <if test="quantity != null "> and quantity = #{quantity}</if>
            <if test="price != null  and price != ''"> and price = #{price}</if>
            <if test="referencePrice != null  and referencePrice != ''"> and reference_price = #{referencePrice}</if>
            <if test="taxRate != null  and taxRate != ''"> and tax_rate = #{taxRate}</if>
            <if test="externalOrderNo != null  and externalOrderNo != ''"> and external_order_no = #{externalOrderNo}</if>
            <if test="deliveryTime != null "> and delivery_time = #{deliveryTime}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="quantityReceived != null "> and quantity_received = #{quantityReceived}</if>
            <if test="isShelf != null "> and is_shelf = #{isShelf}</if>
        </where>
    </select>
    
    <select id="selectPurchaseDetailById" parameterType="Long" resultMap="PurchaseDetailResult">
        <include refid="selectPurchaseDetailVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertPurchaseDetail" parameterType="PurchaseDetail" useGeneratedKeys="true" keyProperty="id">
        insert into purchase_detail
        <trim prefix="(" suffix=")" suffixOverrides=",">
